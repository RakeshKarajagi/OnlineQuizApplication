<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.quizapp.controllers.RegistrationController">
    <TextField fx:id="usernameField" layoutX="200" layoutY="100" promptText="Username"/>
    <PasswordField fx:id="passwordField" layoutX="200" layoutY="150" promptText="Password"/>
    <Button text="Register" layoutX="200" layoutY="200" onAction="#register" fx:id="registerButton"/>
    <Button text="Back" layoutX="300" layoutY="200" onAction="#goBack" fx:id="backButton"/>
</AnchorPane>
